{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "modules/calculateprice.ts", // Sp√©cifiez votre fichier TypeScript ici
    "**/*.ts",
    "**/*.tsx",
    "components/CigfreeSrc/CigfreeApp.js"
, "components/FolioNavAndRedirection.tsx"  ],
  "exclude": ["node_modules"]
}
